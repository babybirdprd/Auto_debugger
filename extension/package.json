{
  "name": "vscode-react",
  "displayName": "vscode-react",
  "description": "Inception",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.viewconfig"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "dummyconfig",
        "aliases": [
          "DummyConfig",
          "dc"
        ],
        "extensions": [
          ".json"
        ]
      }
    ],
    "commands": [
      {
        "command": "extension.viewconfig",
        "title": "Open Chat",
        "category": "AutoDebugger"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm-run-all compile:*",
    "watch": "npm-run-all -p watch:*",
    "compile:extension": "tsc -p ./",
    "compile:views": "webpack --mode development --stats-error-details",
    "watch:extension": "tsc -watch -p ./",
    "watch:views": "webpack --watch --mode development",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.17.60",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/vscode": "^1.89.0",
    "autoprefixer": "^10.4.19",
    "css-loader": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.5.12",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.3",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "tslint": "^5.12.1",
    "typescript": "^5.4.5",
    "vscode": "^1.1.28",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@uiw/codemirror-theme-monokai-dimmed": "^4.22.0",
    "@uiw/react-codemirror": "^4.22.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "fsevents": "^2.3.3",
    "groq-sdk": "^0.3.3",
    "lucide-react": "^0.378.0",
    "next": "^14.2.3",
    "openai": "^4.46.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  }
}
