{
  "name": "simple-extension",
  "displayName": "Simple Extension",
  "description": "A simple VS Code extension",
  "version": "0.1.0",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": "^1.50.0"
  },
  "main": "./extension.js",
  "activationEvents": [
    "onCommand:simple-extension.helloWorld"
  ],
  "contributes": {
    "commands": [
      {
        "command": "simple-extension.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "yourExtension.showChat",
        "title": "Auto_debugger Show Chat",
        "category": "Your Extension"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "vscode": "^1.1.36",
    "eslint": "^7.1.0",
    "typescript": "^4.0.3"
  }
}